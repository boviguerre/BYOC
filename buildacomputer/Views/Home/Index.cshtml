@using System.Collections.Generic;


<body>
    <h3 class="font-styling">Recently Built Computers</h3>
    <div>
        <table class="table">
            <tr>
                @foreach (string key in Model.Keys)
                {
                    <th><a href="#">@key</a></th>
                }
            </tr>
            @for (int i = 0; i < Model["Motherboard"].Count; i++)
            {
                <tr>
                    @foreach (string key in Model.Keys)
                    {
                        <td>@Model[key][i]</td>
                    }
                </tr>
            }
        </table>
    </div>

    <div class="jumbotron">
        <h1></h1>
        <p class="lead"></p>
    </div>
    <div class="jumbotron">
        <h1></h1>
        <p class="lead"></p>
    </div>

</body>

